  #xthickness            			= 3
  #ythickness            			= 2
    
  fg[NORMAL]			= "#000000"
  fg[PRELIGHT]			= "#000000"
  fg[ACTIVE]				= "#000000"
  fg[SELECTED]			= "#000000"
  fg[INSENSITIVE]			= "#A8A8A8"
  
#[window background]
