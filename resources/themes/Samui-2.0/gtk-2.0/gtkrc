    }
    image
    {
      function			= HANDLE
      recolorable			= TRUE

      overlay_file		= "Handles/handle.png"
      overlay_stretch		= FALSE
      orientation			= VERTICAL
    }

    # shadow

    image
    {
      function			= SHADOW
      shadow			= IN
      recolorable			= FALSE
      file				= "Shadows/shadow-in.png"
