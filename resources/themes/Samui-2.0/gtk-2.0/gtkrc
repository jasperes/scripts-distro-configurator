      recolorable			= TRUE
	shadow			= IN
      state				= NORMAL
      file				= "Shadows/entry-shadow-in.png"
      border			= { 6, 6, 6, 6 }
      stretch			= TRUE
    }	

image
    {
      function			= BOX
      recolorable			= TRUE
	shadow			= OUT
      state				= NORMAL
      file				= "Shadows/text-entry.png"
