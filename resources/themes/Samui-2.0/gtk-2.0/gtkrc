   {
     function			= BOX
     state				= INSENSITIVE
     file				= "Buttons/button-insensitive.png"
      border			= { 8, 8, 8, 8 }
     stretch			= TRUE
    }
    image 
    {
      function			= BOX
      file				= "Buttons/button-normal.png"		
      border			= { 8, 8, 8, 8 }
      stretch			= TRUE
    }	
  }
