{

  xthickness            			= 0
  ythickness            			= 2

  engine "pixmap"
  {
    image
    {
      function			= BOX
      recolorable			= TRUE
      state				= PRELIGHT
      file				= "Combo/option-button-prelight.png"
      border			= { 4, 22, 4, 4}
      stretch			= TRUE
    }
    
    image
    {
      function			= BOX
      recolorable			= TRUE
      state				= NORMAL
      file				= "Combo/option-button-normal.png"
      border			= { 4, 22, 4, 4}
      stretch			= TRUE
    }
     
   image
    {
      function			= BOX
      recolorable			= TRUE
      state				= ACTIVE
      file				= "Combo/option-button-active.png"
      border			= { 4, 22, 4, 4}
      stretch			= TRUE
    }
  
    image
    {
      function			= BOX
      recolorable			= TRUE
      state				= INSENSITIVE
      file				= "Combo/option-button-insens.png"
      border			= { 4, 22, 4, 4}
      stretch			= TRUE
    }

 image
    {
      function			= TAB
      state				= NORMAL
      recolorable			= TRUE
      overlay_file		= "Combo/option-menu-arrow.png"
      overlay_stretch		= FALSE
    }

 image
    {
      function			= TAB
      state				= PRELIGHT
      recolorable			= TRUE
      overlay_file		= "Combo/option-menu-arrow-prelight.png"
      overlay_stretch		= FALSE
    }
 image
    {
      function			= TAB
      state				= ACTIVE
      recolorable			= TRUE
      overlay_file		= "Combo/option-menu-arrow-active.png"
      overlay_stretch		= FALSE
    }

    image
    {
      function			= TAB
      state				= INSENSITIVE
      recolorable			= TRUE
      overlay_file		= "Combo/option-menu-arrow-insens.png"
      overlay_stretch		= FALSE
    }
   
    image
    {
      function			= TAB
      recolorable			= TRUE
      overlay_file		= "Combo/option-menu-arrow.png"
      overlay_border		= { 0, 0, 0, 0 }
      overlay_stretch		= FALSE
    }
  }
}

############ Checkboxes ##########


style "checkbutton"	= "default"
{
  engine "pixmap" 
    {
	#This is the image used to draw an unchecked box.
        image 
	{
            function         = CHECK
            recolorable     = TRUE
            state		 = NORMAL
            shadow          = OUT
            overlay_file     = "Check-Radio/checkbox-unselected-normal.png"
            overlay_stretch = FALSE
        }

 image 
	{
            function        = CHECK
            recolorable     = TRUE
            state 		= PRELIGHT
            shadow         = OUT
            overlay_file    = "Check-Radio/checkbox-unselected-prelight.png"
            overlay_stretch = FALSE
        }

 image 
	{
            function         = CHECK
            recolorable     = TRUE
            state 		 = ACTIVE
            shadow          = OUT
            overlay_file    = "Check-Radio/checkbox-unselected-pressed.png"
            overlay_stretch = FALSE
        }

 image 
	{
            function         = CHECK
            recolorable     = TRUE
            state 		 = INSENSITIVE
            shadow          = OUT
            overlay_file    = "Check-Radio/checkbox-disabled-normal.png"
            overlay_stretch = FALSE
        }






        image 
	{
            function         = CHECK
            recolorable     = TRUE
	    state 		= NORMAL
            shadow          = IN
            overlay_file    = "Check-Radio/checkbox-selected-normal.png"
            overlay_stretch = FALSE
        }

  image 
	{
            function        = CHECK
            recolorable     = TRUE
	    state 		= PRELIGHT
            shadow          = IN
            overlay_file    = "Check-Radio/checkbox-selected-prelight.png"
            overlay_stretch = FALSE
        }

 image 
	{
            function        = CHECK
            recolorable     = TRUE
	    state 		= ACTIVE
            shadow          = IN
            overlay_file    = "Check-Radio/checkbox-selected-pressed.png"
            overlay_stretch = FALSE
        }


     image 
	{
            function        = CHECK
            recolorable     = TRUE
	    state 		= INSENSITIVE
            shadow          = IN
            overlay_file    = "Check-Radio/checkbox-disabled-normal.png"
            overlay_stretch = FALSE
        }


	image 
	{
          function           = FLAT_BOX
          recolorable     	= TRUE
      	  stretch		= TRUE
          file                  = "Check-Radio/checklight.png"
          border             = { 2, 2, 2, 2 }
        }

    }
}
